.top-nav {
  width: 100%;
  background: #fff;
}
.top-nav .top-nav-wrap {
  width: 1240px;
  margin: 0 auto;
  padding-top: 10px;
  padding-bottom: 7px;
  height: 30px;
  line-height: 30px;
  color: #4a4a4a;
  font-size: 12px;
}
.top-nav .top-nav-wrap span.left-line {
  margin-left: 5px;
  color: #dfdfdf;
}
.top-nav .top-nav-wrap .logo {
  display: block;
  float: left;
  width: 75px;
  height: 28px;
  margin-right: 30px;
  background: url(../images/logo.png) no-repeat;
}
.top-nav .top-nav-wrap .welcome-wrap {
  float: left;
  color: #808080;
}
.top-nav .top-nav-wrap .welcome-wrap .user-name,
.top-nav .top-nav-wrap .welcome-wrap .link {
  color: #808080;
  margin-left: 5px;
}
.top-nav .top-nav-wrap .welcome-wrap .user-name:hover,
.top-nav .top-nav-wrap .welcome-wrap .link:hover {
  color: #e69b02;
}
.top-nav .top-nav-wrap ul.right-menu {
  float: right;
}
.top-nav .top-nav-wrap ul.right-menu li {
  float: left;
  padding: 0 14px;
  position: relative;
  z-index: 1;
}
.top-nav .top-nav-wrap ul.right-menu li a {
  color: #4a4a4a;
}
.top-nav .top-nav-wrap ul.right-menu li a:hover {
  color: #e69b02;
}
.top-nav .top-nav-wrap ul.right-menu li .line {
  position: absolute;
  right: 0;
  top: 11px;
  z-index: 2;
  background: #dfdfdf;
  height: 10px;
  width: 1px;
}
.top-nav .top-nav-wrap ul.right-menu li .cart-num {
  display: block;
  position: absolute;
  top: 4px;
  right: 3px;
  width: 13px;
  height: 13px;
  line-height: 13px;
  text-align: center;
  background: url(../images/circle.png) no-repeat;
  color: #fff;
}
