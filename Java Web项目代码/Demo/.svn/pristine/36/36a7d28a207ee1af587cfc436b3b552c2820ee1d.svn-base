/* 下拉框 */
.combo-box {
  position: absolute;
  color: #646464;
  overflow: hidden;
  zoom: 100%;
  border: 1px solid #d9d9d9;
}
.combo-box .combo-box-top {
  padding: 0 4px;
  font-size: 14px;
  line-height: 24px;
  height: 24px;
  cursor: pointer;
  color: #a0a0a0;
}
.combo-box .fir-txt {
  text-align: right;
  color: #646464;
}
.combo-box .down-mark {
  float: right;
  display: block;
  height: 7px;
  width: 10px;
  margin-top: 9px;
  margin-left: 12px;
  background: url('../images/arrow-down1.png') no-repeat center center;
}
.combo-box ul {
  z-index: 998;
  display: none;
  border-top: 1px solid #e5e5e5;
  position: relative;
  max-height: 200px!important;
  height: auto!important;
  overflow: auto;
  background: #fff;
}
.combo-box ul li {
  float: none!important;
  height: 30px;
  padding: 0 4px;
  line-height: 30px!important;
  font-size: 14px;
  margin: 0!important;
  text-align: left;
}
.combo-box ul li:hover {
  background: #e69b02;
  color: #fff;
  cursor: pointer;
  -moz-opacity: 0.8;
  opacity: 0.8;
  filter: alpha(opacity = 80);
}
/* 复选框 单选框 */
label {
  display: block;
  cursor: pointer;
  line-height: 26px;
  min-width: 19px;
  height: 20px;
  float: left;
  color: #5e5e5e;
}
.radios {
  padding-top: 18px;
  border-top: 1px solid #049CDB;
}
.label_check input,
.label_radio input {
  margin-right: 5px;
}
.label_radio,
.label_check {
  background: #ffffff url(../images/radioCheck3.png) no-repeat;
}
.label_check {
  background-position: 0 0px;
}
label.c_on {
  background-position: 0 -21px;
  color:#EE4E4E;
}
.label_radio {
  background-position: 0 -41px;
}
.withTxt {
  padding-left: 17px;
  line-height: 12px;
  font-size: 12px;
}
label.r_on {
  background-position: 0 -62px;
}
.label_check input,
.label_radio input {
  position: absolute;
  left: -9999px;
}
/* 文本框 */
.input-txt {
  border: 1px solid #d9d9d9;
  height: 35px;
  line-height: 35px;
  padding: 2px 15px;
  outline: none;
 /*color: #a0a0a0;*/
  font-size: 14px;
}
.input-txtarea {
  border: 1px solid #d9d9d9;
  line-height: 18px;
  resize: none;
  padding: 5px 10px;
