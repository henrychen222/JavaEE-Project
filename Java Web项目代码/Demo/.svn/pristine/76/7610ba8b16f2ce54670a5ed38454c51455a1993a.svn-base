.menu-right{
		width: 200px;
		border-radius:5px;
		border:0.2px solid #ddd;
		overflow: hidden;
		padding-bottom: 1px;
	}
	
	.list-group{
		margin-bottom: 0px;
	}
	button.list-group-item{
		width: 100%;
		text-align: left;
		outline:none;
		blr:expression(this.onFocus=this.blur());
	}
	:last-child.list-group-item{
		border-bottom-right-radius:0px;
		border-bottom-left-radius:0px;
	}
	:first-child.list-group-item{
		border-top-left-radius:0px;
		border-top-right-radius:0px;
	}
	.list-group-item{
		height: 43px;
	}
	.list-group-item.parent{
		background-color:#fdf0db;
	}
	.menu-list a.list-group-item:hover, .menu-list a.list-group-item:focus{
		background-color: #6a1717;
		color: #fff;
		border:0px;
	}
	
	.sub-menu a.list-group-item.subactive, .sub-menu a.list-group-item:hover, .sub-menu a.list-group-item:focus{
		background-color: #eee;
		color: #777;
	}
	
	
	.list-group div[class*="subMenu"]{
		display: none;
	} 
	
	.menu-list a.list-group-item.active, .menu-list a.list-group-item.active:hover, .menu-list a.list-group-item.active:focus{
		background-color: #6a1717;
		color: #fff;
		border:0px;
	}
	
	.list-group-item.factive{
		background-color: #6a1717;
		color: #fff;
		border:0px;
	}
