<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BaseMenu">
	<resultMap type="com.zbiti.iepe.framework.model.BaseMenu" id="BaseMenuResultMap">
		<id property="menuId" column="menu_id"/>
		<result property="name" column="name"/>
		<result property="title" column="title"/>
		<result property="parentMenuId" column="parent_menu_id"/>
		<result property="uri" column="uri"/>
		<result property="icon" column="icon"/>
		<result property="menuTypeCd" column="Menu_type_cd"/>
		<result property="menuSortCd" column="Menu_sort_cd"/>
		<result property="desc" column="menu_desc"/>
	</resultMap>
	
</mapper>
