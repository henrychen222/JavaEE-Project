package com.jd.util;

public class StringUtil {
	
	public static String countChar(String str){
        StringBuffer sb = new StringBuffer("");
        for (int i = 0; i < str.length(); i++) {   
            char c = str.charAt(i);   
            if ((c >= 'a' && c<='z') || (c >= 'A' && c<='Z')) {   
            	sb.append(c);
            } 
        }   
        return sb.toString(); 
	}

