.top-nav{ background: #f7f7f7;
    .top-nav-wrap{ padding: 0;}
}
.header{ width: 1240px; margin: 0 auto; padding: 13px 0;
	.logo-wrap{ float: left;
        .logo{ display: block; width: 172px; height: 65px; float: left; background: url(../images/logo2.png) no-repeat; }
        .logo-tip{width: 123px; height: 34px; margin:23px 0 0 23px; float: left; background: url(../images/logo2-right.png) no-repeat;}
    }
    /* 搜索bar */
    .search-bar-wrap{  float: left; margin-left: 56px; margin-top: 16px; border:0;
        .input-txt{ float: left;height: 28px;  line-height: 28px;text-align: left; color: #adadad;border: 2px solid #e69b02; border-right:0;}
        a.sear-btn, a.sear-shop-btn{ display: block; padding: 0 13px; height:32px; line-height: 32px; float: left; background: #e69b02; color: #fff; font-size: 18px;border: 2px solid #e69b02;}
        a.sear-btn:hover, a.sear-shop-btn:hover{ background: #123457; border-color:#123457;}
        a.sear-shop-btn{ margin-left:2px;}
    }
    .right-infor{ float: right;
        ul{
            li{ float: left; display: block; background: #f9f9f9; width: 139px; height: 50px; margin-left: 2px; margin-top: 9px;
                p{ text-align: center; font-size: 12px; color: #a0a0a0; font-family: "SimSun";}
                p.num{ font-weight: bold; font-size: 24px; font-family: "Microsoft YaHei"; color: #ee4e4e; margin-top: 4px; line-height: 26px;
                    img{ margin: -3px 0 0 5px;}
                }
            }
        }
    }
}

.center{ width:1240px; margin: 7px auto 0; clear: both; position: relative;
    .nav-wrap{
        .left-menu-wrap{ float: left; width: 237px; position: relative;
            .tit{ background: #123457; cursor: pointer; color: #fff; text-align: center; height: 40px; line-height: 40px; font-size: 18px;}
            .content{ position: absolute; top: 40px; left: 0; z-index: 999;width: 237px; 
                ul{
                    li{ font-size: 14px; cursor: pointer; clear: both;}
                    li.menu-tit{ background: #1a426b; color: #d1e8ff; text-align: left; padding-left: 75px; height: 25px; line-height: 25px; position: relative;
                        a{ float: right; color: #6f9bc7; font-size: 12px;}
                        a:hover{ color: #123457;}
                        span.right-mark{ display: block; float: right; width: 6px; height: 10px; margin: 8px 15px 0 5px; background: url(../images/arrow-right2.png) no-repeat;}
                    }
                    li#moreType:hover{background: #f6f6f6; border-color:#f6f6f6; border-right:0; color:#123457;}
                    li.menu-item{ position: relative; height: 32px; line-height: 32px; background: #184c81; color: #fff;border:1px solid #184c81; 
                        span.txt{ float: left;}
                        span.menu-icon{ display: block; width: 22px; height: 18px; margin-top: 11px; margin-left: 13px; margin-right: 18px; float: left; background: url(../images/nav-icon.png) no-repeat;}
                        span.right-mark{ display: block; float: right; width: 6px; height: 10px; margin: 13px 15px 0 0; background: url(../images/arrow-right2.png) no-repeat;}
                        div.mask{ position: absolute; display: none; left: 234px; top: 1px; width: 1px; height:32px; background: #f6f6f6; z-index: 2; }
                        span.menu-icon1{ background-position: 0 0;}
                        span.menu-icon2{ background-position: 0 -31px;}
                        span.menu-icon3{ background-position: 0 -65px;}
                        span.menu-icon4{ background-position: 0 -104px;}
                        span.menu-icon5{ background-position: 0 -138px;}
                        span.menu-icon6{ background-position: 0 -171px;}
                        span.menu-icon7{ background-position: 0 -226px;}
                        span.menu-icon8{ background-position: 0 -262px;}
                        span.menu-icon9{ background-position: 0 -296px;}
                        span.menu-icon10{ background-position: 0 -329px;}
                        span.menu-icon11{ background-position: 0 -364px;}
                        span.menu-icon12{ background-position: 0 -395px;}
                        span.menu-icon13{ background-position: 0 -434px;}
                    }
                    .inner-wrap{ display: none; position: absolute; width: 383px; height: 505px; left: 236px; top: -1px; z-index: 1; border-top:1px solid #123457;background: #f6f6f6;
                            ul{ float: left;
                                li{ padding-left: 15px; cursor: default;
                                    a{ color: #696969; font-size: 12px; font-weight: normal; line-height: 40px;}
                                    a:hover{ color: #e69b02; font-weight: bold;}
                                    span.line{ display: block; float: right; width: 1px; height: 10px; background: #dfdfdf; margin: 15px 0 0 8px;}
                                }
                            }
                            .right-shop{ float: right; margin-right: 14px; margin-top: 15px;
                                a{ display: block; float: left; width: 63px; height: 20px; padding: 10px; margin-right: 1px; margin-bottom: 1px; background: #ededed;
                                    img{ display: block; width: 55px;}
                                }
                                .pro-hot{ width: 167px; height: 150px;
                                    a{ display: block; width: 100%; height: auto; padding: 0;
                                        p{ line-height: 18px;}
                                        p.blue{ color: #1a426b; font-size: 14px; margin-top: 12px; margin-left: 10px;}
                                        p.grey{ color: #868686; font-size: 12px; margin-left: 10px;}
                                        img{ float: right; clear: both; margin-right: 15px; margin-bottom: 25px; margin-top: -2px;}
                                    }
                                }
                                a:hover{ background: #e1dcdc;}
                            }
                        }
                    li#moreType{
                        .inner-wrap{ left: 237px;}
                    }
                    li.menu-item:hover{ background: #f6f6f6; border-color:#f6f6f6; border-right:0; color:#123457;}
                    li.menu-item:hover span.menu-icon{background: url(../images/nav-icon-on.png) no-repeat;}
                    li.menu-item:hover span.menu-icon1{ background-position: 0 0;}
                    li.menu-item:hover span.menu-icon2{ background-position: 0 -31px;}
                    li.menu-item:hover span.menu-icon3{ background-position: 0 -65px;}
                    li.menu-item:hover span.menu-icon4{ background-position: 0 -104px;}
                    li.menu-item:hover span.menu-icon5{ background-position: 0 -138px;}
                    li.menu-item:hover span.menu-icon6{ background-position: 0 -171px;}
                    li.menu-item:hover span.menu-icon7{ background-position: 0 -226px;}
                    li.menu-item:hover span.menu-icon8{ background-position: 0 -262px;}
                    li.menu-item:hover span.menu-icon9{ background-position: 0 -296px;}
                    li.menu-item:hover span.menu-icon10{ background-position: 0 -329px;}
                    li.menu-item:hover span.menu-icon11{ background-position: 0 -364px;}
                    li.menu-item:hover span.menu-icon12{ background-position: 0 -395px;}
                    li.menu-item:hover span.menu-icon13{ background-position: 0 -434px;}
                    li.menu-item1 .inner-wrap{ top:-27px;}
                    li.menu-item2 .inner-wrap{top:-61px;}
                    li.menu-item3 .inner-wrap{top:-95px;}
                    li.menu-item4 .inner-wrap{top:-129px;}
                    li.menu-item5 .inner-wrap{top:-163px;}
                    li.menu-item6 .inner-wrap{top:-197px;}
                    li.menu-tit .inner-wrap{top:-230px;}
                }
            }
        }
        .right-cont-wrap{ float: left; width: 1003px; position: relative;
            .nav-wrap-inner{ width: 100%; border-bottom: 1px solid #123457;
                ul.left-nav{ float: left;
                    li{ float: left; margin-right: 5px; position: relative;
                        a{ display: block; position: relative; text-align: center; height: 39px; line-height: 40px; width: 108px; font-size: 18px; color: #000;
                            i.icon-new{ position: absolute; top: 2px; right: 8px; width: 27px; height: 17px; background: url(../images/angle-icon-new.png) no-repeat;}
                            i.icon-hot{ position: absolute; top: 2px; right: 0; width: 27px; height: 17px; background: url(../images/angle-icon-hot.png) no-repeat;}
                        }
                        a:hover{background: #e69b02; color: #fff;}
                        ul{ position: absolute; top: 39px; left: 0; z-index: 5; display: none;
                            li{ border-top: 1px solid #f6bc46;
                                a{background:#e69b02!important; color: #000!important; }
                            }
                        }
                    }
                    li.active{
                        a{ background: #e69b02; color: #fff;}
                    }
                }
            }
        }
    }
    .sear-condit{ border-bottom:1px solid #e3e3e3; margin-top: 20px;
        .list{ line-height: 48px; border:1px solid #e3e3e3; border-bottom: 0;
            .tit{ width: 90px;float: left;margin-left: 15px;font-size: 12px;color: #a0a0a0;}
            .cont{ float: left; width:1088px; position: relative; margin-left: 45px;
                ul.typelist{ float: left;
                    li{ float: left; display: inline-block; min-width: 110px; text-align: left;
                        a{ font-size: 12px; color: #646464;}
                        a:hover{ color: #e69b02;}
                    }
                    li.active a{font-size: 14px;}
                }
                a.more-type{position: absolute; top: 0; right: 0; margin-right: 10px; font-size: 12px; color: #646464; font-family: "SimSun"}
                a.more-type:hover{ color: #e69b02;}
            }
        }
        .btn-wrap{ height: 54px; font-size: 12px; text-align: center; line-height: 54px;
            .btn-ok{ display: inline-block; background: #003366; margin-right: 27px; color: #fff; height: 26px; line-height: 26px; width: 73px; text-align: center; }
            .btn-ok:hover{ background: #0c4682;}
            .btn-cancel{ display: inline-block; background: #f4f4f4; color: #000; height: 26px; line-height: 26px; width: 73px; text-align: center; border: 1px solid #a3a3a3;}
            .btn-cancel:hover{ color: #003366; border-color: #003366;}
        }
    }

    .pro-cont{
        .pro-cont-tit{ font-size: 14px; color: #000; height: 50px; line-height: 50px; font-weight: normal;}
        .pro-cont-inner{border-bottom: 1px solid #e3e3e3;
            ul{
                li.pro-bid-list-wrap{
                    .pro-bid-list{ padding: 5px 15px; border: 1px solid #e3e3e3; border-bottom: 0; overflow: hidden;
                        .pro-bid-tit{ font-size: 14px; color: #000; line-height: 30px;
                            span{ width: 155px; overflow: hidden;}
                        }
                        ul{ clear: both;
                            li{ color: #a0a0a0; font-size: 12px; float: left; line-height: 26px;}
                            li.pro-bid-des{ width: 475px; margin-bottom: 8px;
                                span{ width: 335px; overflow: hidden;}
                            }
                            li.pro-bid-price{ width: 142px; padding-left: 33px;
                                span{ color: #ee4e4e;
                                    i{ font-style: normal; font-size: 18px;}
                                }
                            }
                            li.pro-bid-status{ width: 142px; padding-left: 33px;
                                span{ color: #000;}
                            }
                            li.pro-bid-starttime{ width: 200px; padding-left: 40px;
                                span{ color: #000;}
                            }
                            li.pro-bid-startper{ width: 106px; padding-left: 35px;
                                span{ color: #000;}
                            }
                            li.pro-bid-tag{ width: 830px; margin-bottom: 10px;
                                a{ display: block; float: left; margin-right: 5px; width: 60px; height: 23px; border:1px solid #a3a3a3; text-align: center; line-height: 23px; font-size: 12px; color: #646464;}
                            }
                            li.pro-bid-endtime{ padding-left: 35px;
                                span{ color: #000;}
                            }
                        }
                    }
                }
            }
        }
    }


    .page-nav{ margin: 22px auto 30px;}
}
