// 用户注册页面
.top-nav{ background: #f7f7f7;
    .top-nav-wrap{ padding: 0;}
}
.header{ width: 1140px; margin: 0 auto; padding: 10px 0;
	h2.slogan{ color: #123457; font-size: 22px; font-weight: normal; padding: 20px 0 10px; border-bottom: 1px solid #e5e5e5;}
}

.center{ width:1140px; margin: 22px auto; clear: both;
    ul{  border-bottom:1px solid #e5e5e5; padding-bottom: 50px; margin-bottom: 10px;
    	li{ clear: both; overflow: hidden; margin-bottom: 20px;
    		.left-tit{ float: left; text-align: left; width: 110px; color: #292929; font-size: 15px; line-height: 35px;}
    		.center-cont{ float: left;

    		}
    		.right-tip{ float: left;}
    	}
    }
    .tip-txt{ color: #a0a0a0; font-size: 12px; line-height: 35px; margin-left: 15px;}
    .get-code{ display: block; background: #f2cd80; color: #fff; font-size: 15px; line-height: 37px; text-align: center;
    	border-radius: 8px; padding: 0 20px; height: 37px; margin-left: 20px;}
	.serv-txt{ width: 1000px; max-height: 300px; overflow: auto; color: #292929; font-size: 13px; line-height: 26px; border:1px solid #e5e5e5; padding: 10px; background: #f8f8f8; }    	
	.btn-regist{display: block; background: #123457; color: #fff; font-size: 16px; line-height: 50px; text-align: center;
    	border-radius: 8px; padding: 0 85px; height: 50px; border:0; margin:30px auto 80px;}
    .btn-regist:hover{ background: #1a4169;}
    .pwd-wrap{ position: relative;
    	span.icon-pwd{position: absolute; display: block; width: 15px; height: 14px; top: 13px; right: 20px; background: url(../images/icon-pwd.png) no-repeat;}
    }
}
