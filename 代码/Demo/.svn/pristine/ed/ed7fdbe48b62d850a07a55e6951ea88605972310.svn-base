.top-nav{ background: #f7f7f7;
    .top-nav-wrap{ padding: 0;}
}
.header{ width: 1240px; margin: 0 auto; padding: 13px 0;
    .logo-wrap{ float: left;
        .logo{ display: block; width: 172px; height: 65px; float: left; background: url(../jhShop/logo2.png) no-repeat; }
        .logo-tip{width: 123px; height: 34px; margin:23px 0 0 23px; float: left; background: url(../jhShop/logo2-right.png) no-repeat;}
    }
    /* 搜索bar */
    .search-bar-wrap{  float: left; margin-left: 56px; margin-top: 16px; border:0;
        .input-txt{ float: left;height: 28px;  line-height: 28px;text-align: left; color: #adadad;border: 2px solid #e69b02; border-right:0;}
        a.sear-btn, a.sear-shop-btn{ display: block; padding: 0 13px; height:32px; line-height: 32px; float: left; background: #e69b02; color: #fff; font-size: 18px;border: 2px solid #e69b02;}
        a.sear-btn:hover, a.sear-shop-btn:hover{ background: #123457; border-color:#123457;}
        a.sear-shop-btn{ margin-left:2px;}
    }
    .right-infor{ float: right;
        ul{
            li{ float: left; display: block; background: #f9f9f9; width: 139px; height: 50px; margin-left: 2px; margin-top: 9px;
                p{ text-align: center; font-size: 12px; color: #a0a0a0; font-family: "SimSun";}
                p.num{ font-weight: bold; font-size: 24px; font-family: "Microsoft YaHei"; color: #ee4e4e; margin-top: 4px; line-height: 26px;
                    img{ margin: -3px 0 0 5px;}
                }
            }
        }
    }
}

.center{ width:1240px; margin: 20px auto 0; clear: both; overflow: hidden; border-top: 1px solid #123457;
    .address-wrap-left{ width: 892px; float: left;
        .address-top{ height: 47px; line-height: 47px; color: #000; font-size: 14px; border-bottom: 1px solid #e3e3e3;
            h2{ font-size: 14px; font-weight: normal; float: left;}
            a{ float: right; font-size: 12px; color: #123457;}
            a:hover{ color: #e69b02;}
        }
        .add-cont{ margin-top: 14px;
            ul{
                li{ clear: both; overflow: hidden; line-height: 26px; padding: 0 15px; margin: 3px 0; cursor: default;
                    .addre-icon{ display: none;float: left; margin-right: 20px;}
                    .addre-txt{ display: none;float: left; margin-right: 33px;}
                    .sele-icon-off{ display: block; margin-top: 7px; margin-left: 112px; float: left; margin-right: 25px; width: 13px; height: 13px; background: url(../jhShop/radio-off.png) no-repeat; }
                    .addre-infor{float: left; color: #383838; font-size: 12px;}
                    .opt-modify{ display: none; float: right; font-size: 12px;}
                    .tel{float: left; color: #a0a0a0; font-weight:normal; font-size: 12px; margin-left: 28px; }
                    .addre-defa{float: left; color: #646464; font-size: 12px; margin-left: 20px; }
                    .clear-no-height{ display: none;}
                }
                li.active{ background: #e1eed2; line-height: 26px; padding: 15px;
                    .addre-icon{ display: block;}
                    .addre-txt{display: block;}
                    .sele-icon-on{display: block; margin-top: 7px; float: left; width: 13px;margin-right: 25px;  height: 13px; background: url(../jhShop/radio-on.png) no-repeat;}
                    .addre-infor{color: #292929;font-size: 14px; }
                    .opt-modify{display: block;}
                    .opt-modify:hover{color: #e69b02;}
                    .tel{ color: #646464; font-weight:bold;margin-left: 148px; font-size: 14px; margin-right: 18px;}
                    .addre-defa{}
                    .clear-no-height{ display: block;}
                }
                li:hover{ background: #e1eed2;}
                li:hover .opt-modify{ display: block; }
            }
            .new-addr{ display: block; margin-left: 127px; margin-top: 15px;  background: #f2f2f2; color: #6c6c6c; font-size: 12px; border:1px solid #d9d9d9; width: 146px; height: 38px; line-height: 38px; text-align: center; 
                img{ margin-right: 7px;}
            }
            .new-addr:hover{ color: #e69b02;}
        }
    }
    .shop-infor-right{ width: 308px; float: right; border:1px solid #dfdfdf; margin-top: 47px;
        h2{ background: #dfdfdf; height: 33px; line-height: 33px; text-align: center; color: #292929; font-size: 16px;}
        .cont{
            p.time{ height: 40px; line-height: 40px; margin: 0 15px; border-bottom: 1px solid #dfdfdf;
                .tit{ color: #000000;}
                .txt{ color: #3e3e3e; font-size: 12px; margin-left: 40px;}
            }
            .cont-wrap{ margin: 0 15px;}
            p.contact{ line-height: 35px; color: #000;}
            .tel-cont{ color: #646464; font-size: 12px; line-height: 26px;
                span{ color: #222; margin-left: 30px;}
            }
            .btn-contact{ display: block; margin:15px auto; background: #e69b02; color: #fff; font-weight: bold; text-align: center; width: 240px; height: 38px; line-height: 38px; border-radius: 8px;}
            .btn-contact:hover{ background: #d79006;}
        }
    }
    .order-confirm{ clear: both; height: 152px; padding: 13px; border:1px solid #d9d9d9; background: #f2f2f2; margin-top: 20px;
        h2{ font-size: 14px; font-weight: normal; line-height: 35px;}
        .order-status-inner{ width: 932px; margin: 35px auto;
            div{ position: relative; z-index: 1; float: left; width: 155px; height: 6px; background: url(../jhShop/line-bg.png) repeat-x;
                span.icon-num-off{ z-index: 2; display: block; position: absolute; width: 26px; height: 26px; background: url(../jhShop/order-status-off.png) no-repeat;}
                span.icon-num-on{z-index: 2;display: none; position: absolute; width: 26px; height: 26px; background: url(../jhShop/order-status-on.png) no-repeat;}
                span.step-txt{ position: absolute; top: 16px; right: -40px; display: block; width: 60px; color: #a0a0a0; font-size: 12px;}
            }
            div.status1{ width: 1px;z-index: 7;
                span.icon-num-off, span.icon-num-on{top: -12px; right: -13px; background-position: 0 0; }
            }
            div.status2{z-index: 6;
                span.icon-num-off, span.icon-num-on{top: -12px; right: -13px; background-position: -121px 0; }
            }
            div.status3{z-index: 5;
                span.icon-num-off, span.icon-num-on{top: -12px; right: -13px; background-position: -243px 0; }
            }
            div.status4{z-index: 4;
                span.icon-num-off, span.icon-num-on{top: -12px; right: -13px; background-position: -364px 0; }
            }
            div.status5{z-index: 3;
                span.icon-num-off, span.icon-num-on{top: -12px; right: -13px; background-position: -485px 0; }
            }
            div.status6{z-index: 2;
                span.icon-num-off, span.icon-num-on{top: -12px; right: -13px; background-position: -606px 0; }
            }
            div.status7{ z-index: 1;
                span.icon-num-off, span.icon-num-on{top: -12px; right: -13px; background-position: -728px 0; }
                span.step-txt{ right: -48px;}
            }
            div.active{ background: url(../jhShop/line-on-bg.png) repeat-x;
                span.step-txt{ color: #292929;}
                span.icon-num-off{ display: none;}
                span.icon-num-on{ display: block;}
            }
        }
    }
    .order-tit{ height: 65px; line-height: 65px;
        .left{ float: left; font-size: 12px;
            span{ color: #404040; margin-right: 30px;}
        }
        .right{ float: right;font-size: 12px; color: #292929;
            .btn-choose{ display: block; float: right; margin-top: 19px; margin-left: 10px; width: 105px; height: 24px; line-height: 25px; text-align: center; background: #f4f4f4; border:1px solid #a3a3a3;}
            .btn-choose:hover{ color: #e69b02; border-color: #e69b02;}
        }
    }
    .list-header{ clear: both; height: 32px; line-height: 32px; background: #dfdfdf; color: #292929; font-size: 14px;
        .checkbox{ float: left; margin-left: 11px; width: 14.5%;
            input{ margin-right: 15px;}
        }
        .pro-infor{ float: left; width: 34%;}
        .pro-price{ float: left; width: 11.5%;}
        .amount{ float: left; width: 10%;}
        .param{ float: left; width: 10.5%;}
        .option{ float: left;}
        .status{ float: left; width: 11.5%;}
    }
    .selected-pro-wrap{ font-size:12px; clear: both;
        .order-infor-wrap{ padding: 0 14px; color: #000; font-size: 12px; height: 44px; line-height: 48px;
            .order-shopname{ float: left; margin-right: 65px;
                input{ margin-right: 15px;}
                span{
                    i{ color: #646464; font-style: normal; }
                }
            }
            .order-del{float: left; margin-right: 65px;
                a:hover{ color:#e69b02;}
            }
            .online-ser{float: left; margin-right: 65px;
                img{ width: 20px;}
            }
            .shop-tel{float: left; margin-right: 65px;}
            .shop-addr{
                span{ color: #646464}
            }
        }
        .pro-wrap{ background:#f2f2f2; border:1px solid #e3e3e3; border-bottom: 0; clear: both;
            .pro-top{  padding: 0 24px; border-bottom: 1px solid #e3e3e3;
                ul{
                    li{ border-bottom: 1px solid #e3e3e3; padding: 13px 0; overflow: hidden; font-size: 12px; color: #383838;
                        .checkbox-item{ float: left;}
                        .pro-img{float: left; width: 150px; height: 105px; margin-left: 10px;}
                        .pro-infor{float: left; width: 338px; height: 105px; padding: 0 22px; position: relative;
                            .pro-name{ line-height: 18px; margin-top: 12px; width: 265px;}
                            .pro-serv{ position: absolute; bottom: 0; left: 22px; }
                        }
                        .pro-price{float: left; width: 86px;padding: 0 22px; text-align: center;
                            .new{ color: #ee4e4e; margin-top: 12px; 
                                i{font-size:18px; font-style: normal;}
                            }
                        }
                        .pro-amount{float: left; width:82px;padding: 0 22px; margin-top: 16px;
                            a.num-add, a.num-minus{ display: block; float: left; font-size:18px; width: 22px; height: 22px; line-height: 22px; text-align: center; border-top:1px solid #dedede; border-bottom:1px solid #dedede;}
                            a.num-add:hover, a.num-minus:hover{ background: #e69b02; color: #fff;}
                            a.num-add{ border-right:1px solid #dedede;}
                            .num-detail{ background: #fff; float: left; padding: 0 10px; height: 22px; line-height: 22px; border: 1px solid #dedede; text-align: center;}
                            a.num-minus{ border-left:1px solid #dedede;}
                        }
                        .pro-status{ float: left; margin-top: 16px; width: 120px; text-align: center;}
                        .pro-param{ text-align: center; float: left; width: 121px;padding: 17px 22px; cursor: pointer; }
                        .pro-option{float: left; width: 50px;padding: 18px 22px; text-align: center;
                            a{ display: block; margin-bottom: 8px;}
                        }
                        a:hover{ color: #e69b02}
                    }
                }
                .tip-wrap{ padding: 15px 0; overflow: hidden;
                    .tit{ float: left; margin-right: 5px; line-height: 22px; }
                    .cont{ float: left;
                        a{ text-decoration: underline;}
                        ul{
                            li{ border: 0; padding: 0; line-height: 22px;}
                        }
                    }
                    .left{ float: left;}
                    .right{ float: left; margin-left: 95px;}
                }
            }
            .pro-bottom{ background: #fff; padding: 15px; color: #646464; margin-top: 10px; border-top: 1px solid #e3e3e3; line-height: 22px; overflow: hidden;
                h2{ font-size:14px; font-weight: normal; color: #383838;}
                p{ padding: 0 10px;}
                ul{ padding: 0 10px;}
                ul li{ float: left; width: 150px; margin-right: 145px; border: 0; padding: 0;}
                .pro-size{ margin-top: 5px;}
            }
        }
        .pro-price-wrap{ margin-top: 10px; overflow: hidden;
            .pro-price-inner{ float: right;
                .price-item{ float: left; color: #171717; margin-right: 35px; line-height: 38px;
                    span{ color: #ee4e4e; font-size:12px;
                        i{ font-style: normal; font-size: 18px;}
                    }
                }
                .btn-submit{ float: left; background: #ee4e4e; color: #fff; font-size: 14px; height: 34px; line-height: 34px; text-align: center; border-radius: 8px; width: 120px;}
                .btn-submit:hover{ background: #dd3838;}
            }
        }
        
    }
    .price-bottom{ border:1px solid #d9d9d9; font-size: 12px; text-align: right; margin-top: 20px; padding: 10px; line-height: 22px;
        .pay-fir span{ color: #ed5b5b}
        .pay-time{ color: #a0a0a0;}
    }
    .option-bottom{ margin: 20px 0; clear: both; overflow: hidden;
        .btn-confirm{ display: block; width: 120px; float: right; height: 35px; border-radius: 6px; line-height: 35px; text-align: center; font-size: 14px; background: #ee4e4e; color: #fff;}
        .btn-confirm:hover{ background: #d63838;}
    }
    .print-wrap{ margin: 20px 0 40px; clear: both; position: relative; border: 1px solid #d9d9d9; height: 380px; line-height: 22px;
        .icon-print{ position: absolute; display: block; width:39px; height: 40px; background: url(../jhShop/print.png) no-repeat; right: 12px; top:12px;}
        .cont{ padding: 5px; color: #404040;}
    }
}