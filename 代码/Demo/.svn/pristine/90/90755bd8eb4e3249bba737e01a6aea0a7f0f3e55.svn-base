.top-nav {
  background: #f7f7f7;
  height: 38px;
  line-height: 38px;
}
.top-nav .top-nav-wrap {
  padding: 0;
  height: 38px;
  line-height: 38px;
}
.top-nav .top-nav-wrap .line {
  top: 15px!important;
}
.header {
  width: 680px;
  height:70px;
  margin: 0 auto;
  padding: 10px 0;
  border-bottom:1px solid #CCCCCC;
}
.header h2.slogan {
  color: #123457;
  font-size: 24px;
  font-weight: normal;
  padding: 15px 0 18px;
  border-bottom: 1px solid #e5e5e5;
}
.center {
  width: 680px;
  margin: 0 auto;
  clear: both;
}
.center .desc-info{
	line-height: 25px;
	font-size: 14px;
}

.center ul.form-cont {
  border-bottom: 1px solid #e5e5e5;
}
.center ul.form-cont li {
  clear: both;
  overflow: hidden;
  margin-bottom: 5px;
}
.center ul.form-cont li .left-tit {
  float: left;
  text-align: left;
  width: 110px;
  color: #292929;
  font-size: 14px;
  line-height: 35px;
}
.center ul.form-cont li .center-cont {
  float: left;
}
.center ul.form-cont li .right-tip {
  float: left;
}
.center .tip-txt {
  color: #a0a0a0;
  font-size: 12px;
  line-height: 30px;
  margin-left: 115px;
  font-family: "SimSun";
}
.center .get-code {
  display: block;
  background: #f2cd80;
  color: #fff;
  font-size: 15px;
  line-height: 37px;
  text-align: center;
  border-radius: 8px;
  padding: 0 20px;
  height: 37px;
  margin-left: 20px;
}
.center .serv-txt {
  width: 1000px;
  max-height: 262px;
  overflow: auto;
  color: #292929;
  font-size: 13px;
  line-height: 26px;
  border: 1px solid #e5e5e5;
  padding: 10px;
  background: #f8f8f8;
}
.center .btn-regist {
  display: block;
  background: #123457;
  color: #fff;
  font-size: 16px;
  line-height: 50px;
  text-align: center;
  border-radius: 8px;
  padding: 0 85px;
  height: 50px;
  border: 0;
  margin: 30px auto 80px;
}
.center .btn-regist:hover {
  background: #1a4169;
}
.center .pwd-wrap {
  position: relative;
}
.center .pwd-wrap span.icon-pwd {
  position: absolute;
  display: block;
  width: 15px;
  height: 14px;
  top: 13px;
  right: 20px;
  background: url(../images/icon-pwd.png) no-repeat;
}
.center .dropdown {
  margin-right: 20px;
}
.center .city-drop .combo-box {
  left: 434px;
}



.header .logo-wrap {
  float: left;
}
.header .logo-wrap .logo {
/*   display: block;
  width: 172px;
  height: 65px;
  float: left;
  background: url(../images/logo2.png) no-repeat; */
}
.header .logo-wrap .logo-tip {
  width: 123px;
  height: 34px;
  margin: 23px 0 0 23px;
  float: left;
  background: url(../images/logo2-right.png) no-repeat;
}

.errorRight{
	font-size: 14px;
	margin-left:10px;
	color:red;
}
/* 2015-7-7 */

.center .order-confirm {
  clear: both;
  height: 100px;
  padding: 13px;
  margin-top: 20px;
}
.center .order-confirm h2 {
  font-size: 14px;
  font-weight: normal;
  line-height: 35px;
}
.center .order-confirm .order-status-inner {
  width: 315px;
  margin: 35px auto;
}
.center .order-confirm .order-status-inner div {
  position: relative;
  z-index: 1;
  float: left;
  width: 155px;
  height: 6px;
  background: url(../images/line-bg.png) repeat-x;
}
.center .order-confirm .order-status-inner div span.icon-num-off {
  z-index: 2;
  display: block;
  position: absolute;
  width: 32px;
  height: 32px;
  background: url(../images/order-status-off.png) no-repeat;
}
.center .order-confirm .order-status-inner div span.icon-num-on {
  z-index: 2;
  display: none;
  position: absolute;
  width: 32px;
  height: 32px;
  background: url(../images/order-status-on.png) no-repeat;
}
.center .order-confirm .order-status-inner div span.step-txt {
  position: absolute;
  top: 23px;
  right: -27px;
  display: block;
  width: 60px;
  color: #a0a0a0;
  font-size: 12px;
}
.center .order-confirm .order-status-inner div.status1 {
  width: 1px;
  z-index: 7;
}
.center .order-confirm .order-status-inner div.status1 span.icon-num-off,
.center .order-confirm .order-status-inner div.status1 span.icon-num-on {
  top: -12px;
  right: -13px;
  background-position: 0 0;
}
.center .order-confirm .order-status-inner div.status2 {
  z-index: 6;
}
.center .order-confirm .order-status-inner div.status2 span.icon-num-off,
.center .order-confirm .order-status-inner div.status2 span.icon-num-on {
  top: -12px;
  right: -13px;
  background-position: -121px 0;
}
.center .order-confirm .order-status-inner div.status3 {
  z-index: 5;
}
.center .order-confirm .order-status-inner div.status3 span.icon-num-off,
.center .order-confirm .order-status-inner div.status3 span.icon-num-on {
  top: -12px;
  right: -13px;
  background-position: -243px 0;
}
.center .order-confirm .order-status-inner div.status4 {
  z-index: 4;
}
.center .order-confirm .order-status-inner div.status4 span.icon-num-off,
.center .order-confirm .order-status-inner div.status4 span.icon-num-on {
  top: -12px;
  right: -13px;
  background-position: -364px 0;
}
.center .order-confirm .order-status-inner div.status5 {
  z-index: 3;
}
.center .order-confirm .order-status-inner div.status5 span.icon-num-off,
.center .order-confirm .order-status-inner div.status5 span.icon-num-on {
  top: -12px;
  right: -13px;
  background-position: -485px 0;
}
.center .order-confirm .order-status-inner div.status6 {
  z-index: 2;
}
.center .order-confirm .order-status-inner div.status6 span.icon-num-off,
.center .order-confirm .order-status-inner div.status6 span.icon-num-on {
  top: -12px;
  right: -13px;
  background-position: -606px 0;
}
.center .order-confirm .order-status-inner div.status7 {
  z-index: 1;
}
.center .order-confirm .order-status-inner div.status7 span.icon-num-off,
.center .order-confirm .order-status-inner div.status7 span.icon-num-on {
  top: -12px;
  right: -13px;
  background-position: -728px 0;
}
.center .order-confirm .order-status-inner div.status7 span.step-txt {
  right: -48px;
}
.center .order-confirm .order-status-inner div.active {
  background: url(../images/line-on-bg.png) repeat-x;
}
.center .order-confirm .order-status-inner div.active span.step-txt {
  color: #292929;
}
.center .order-confirm .order-status-inner div.active span.icon-num-off {
  display: none;
}
.center .order-confirm .order-status-inner div.active span.icon-num-on {
  display: block;
}