.center {
  width: 998px;
  margin: 50px auto 0 auto;
  clear: both;
  overflow: hidden;
}
.center .left-menu {
  width: 200px;
  float: left;
}
.center .left-menu ul li a {
  display: block;
  height: 40px;
  background: #fff;
  width: 150px;
  padding: 0 25px;
  border: 1px solid #e3e3e3;
  border-top: 0;
}
.center .left-menu ul li a span.txt {
  color: #a7a7a7;
  font-size: 15px;
  display: block;
  float: left;
  line-height: 40px;
}
.center .left-menu ul li a span.icon-default {
  display: block;
  float: left;
  background: url(../images/icon-left-menu.png) no-repeat;
}
.center .left-menu ul li a span.icon-order {
  height: 16px;
  width: 17px;
  margin: 13px 20px 0 0;
  background-position: -2px -19px;
}
.center .left-menu ul li a span.icon-product {
  width: 16px;
  height: 17px;
  margin: 13px 20px 0 0;
  background-position: -3px -41px;
}
.center .left-menu ul li a span.icon-project {
  width: 17px;
  height: 17px;
  margin: 13px 20px 0 0;
  background-position: -2px -79px;
}
.center .left-menu ul li a span.icon-case {
  width: 16px;
  height: 17px;
  margin: 13px 20px 0 0;
  background-position: -2px -119px;
}
.center .left-menu ul li a span.icon-trust {
  width: 18px;
  height: 18px;
  margin: 13px 20px 0 0;
  background-position: -1px -163px;
}
.center .left-menu ul li a span.icon-engineer {
  width: 20px;
  height: 13px;
  margin: 13px 20px 0 0;
  background-position: 0 -204px;
}
.center .left-menu ul li a span.icon-finance {
  width: 15px;
  height: 18px;
  margin: 13px 20px 0 0;
  background-position: -3px -240px;
}
.center .left-menu ul li a span.icon-supplier {
  width: 18px;
  height: 14px;
  margin: 13px 20px 0 0;
  background-position: -1px -281px;
}
.center .left-menu ul li a span.icon-infor {
  width: 17px;
  height: 20px;
  margin: 13px 20px 0 0;
  background-position: -2px -320px;
}
.center .left-menu ul li a.fir {
  border-radius: 6px 6px 0 0;
  border-top: 1px solid #e3e3e3;
}
.center .left-menu ul li a.las {
  border-radius: 0 0 6px 6px;
}
.center .left-menu ul li a:hover {
  background: #edac29;
  border: 1px solid #edac29;
  border-top: 0;
}
.center .left-menu ul li a:hover span.txt {
  color: #fff;
}
.center .left-menu ul li a:hover span.icon-order {
  background-position: -2px -2px;
}
.center .left-menu ul li a:hover span.icon-product {
  background-position: -3px -60px;
}
.center .left-menu ul li a:hover span.icon-project {
  background-position: -2px -99px;
}
.center .left-menu ul li a:hover span.icon-case {
  background-position: -2px -139px;
}
.center .left-menu ul li a:hover span.icon-trust {
  background-position: -1px -179px;
}
.center .left-menu ul li a:hover span.icon-engineer {
  background-position: 0 -220px;
}
.center .left-menu ul li a:hover span.icon-finance {
  background-position: -3px -260px;
}
.center .left-menu ul li a:hover span.icon-supplier {
  background-position: -1px -298px;
}
.center .left-menu ul li a:hover span.icon-infor {
  background-position: -2px -343px;
}
.center .left-menu ul li.active a {
  background: #e69b02;
  border: 1px solid #e69b02;
  border-top: 0;
}
.center .left-menu ul li.active a span.txt {
  color: #fff;
}
.center .left-menu ul li.active a span.icon-order {
  background-position: -2px -2px;
}
.center .left-menu ul li.active a span.icon-product {
  background-position: -3px -60px;
}
.center .left-menu ul li.active a span.icon-project {
  background-position: -2px -99px;
}
.center .left-menu ul li.active a span.icon-case {
  background-position: -2px -139px;
}
.center .left-menu ul li.active a span.icon-trust {
  background-position: -1px -179px;
}
.center .left-menu ul li.active a span.icon-engineer {
  background-position: 0 -220px;
}
.center .left-menu ul li.active a span.icon-finance {
  background-position: -3px -260px;
}
.center .left-menu ul li.active a span.icon-supplier {
  background-position: -1px -298px;
}
.center .left-menu ul li.active a span.icon-infor {
  background-position: -2px -343px;
}
.center .left-menu ul li.active a.fir {
  border-radius: 6px 6px 0 0;
  border-top: 1px solid #e69b02;
}
